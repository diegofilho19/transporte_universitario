{
  "expo": {
    "name": "transporte_universitario",
    "slug": "transporte_universitario",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/imgs/logo1.png",
    "splash": {
      "image": "",
      "resizeMode": "contain",
      "backgroundColor": "#fafafa"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/imgs/logo1.png",
        "backgroundColor": "#FAFAFA"
      }
    },
    "web": {
      
    },
    "newArchEnabled": true,
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "O aplicativo precisa acessar suas fotos para selecionar a foto do perfil.",
          "cameraPermission": "O aplicativo precisa acessar sua c√¢mera para tirar uma foto do perfil."
        }
      ]
    ]
  }
}